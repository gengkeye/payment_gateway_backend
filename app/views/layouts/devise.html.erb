<body class='unauthenticated'>
  <% flash.each  do |message_type, message| %>
    <div class="alert alert-<%= message_type %>">
      <%= message %>
      <span class='icon-close' id='close_notification_ico'></span>
    </div>
  <% end %>
  <div id='spaceBgContainer'>
  </div>
  <div class='page unauthenticated flexAlign verticalCenter horizontalCenter'>
    <div class='pageContainer'>
      <header>
        <a href="<% root_path %>"><div class='logo'><img src="https://dwsv3vn9of50v.cloudfront.net/assets/mycelium_gear_logo_white-2f97d94d480a2eeeaaf4df6dd5e45d19cd673c6b08821ad3f93592a25073cc55.svg" alt="Mycelium gear logo white 2f97d94d480a2eeeaaf4df6dd5e45d19cd673c6b08821ad3f93592a25073cc55" /></div>
        </a>
      </header>
      <div class='page registration new'>
        <div class='formBlock'>
          <div class='formContent'>
	         <%= yield %>
          </div>
        </div>
      </div>
      <% if devise_controller? %>
        <div class="devise-shared-links">
          <%= render 'users/shared/links' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
</body>
